<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ffff8800" />
            <gradient android:angle="-90" android:startColor="##ffc86b" android:endColor="#ed8b0b"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ffff8800" />
            <solid android:color="#ed8600"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ffff8800" />
            <gradient android:angle="-90" android:startColor="#ff9d42" android:endColor="#ff9d42" />
        </shape>
    </item>
</selector>